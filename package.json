{
  "name": "moepictures",
  "version": "1.0.0",
  "description": "Moepictures is an anime image board for cute/moe art.",
  "main": "dist/server/server.js",
  "scripts": {
    "start": "npm run build && node dist/server/server.js",
    "build": "npm run clean && npm run rsbuild",
    "rsbuild": "rsbuild build --config rsbuild.client.ts && rsbuild build --config rsbuild.server.ts",
    "wasm": "emcmake cmake -DCMAKE_BUILD_TYPE=Release -B structures/build -S structures && cmake --build structures/build",
    "napi": "cmake-js compile",
    "pm2": "pm2 start ecosystem.config.js --env production",
    "ssh": "dotenv -e .env -- sh -c 'ssh -i \"$SSH_KEY_PATH\" \"$SSH_USER\"@\"$SSH_HOST\"'",
    "sitemap": "node sitemap.ts",
    "clean": "del-cli ./dist"
  },
  "binary": {
    "napi_versions": [8]
  },
  "keywords": [
    "moe",
    "cute",
    "kawaii",
    "anime art",
    "image board",
    "booru",
    "moepictures"
  ],
  "repository": {
    "type": "git",
